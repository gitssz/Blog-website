
<%-include("partials/header"); %>
<h1>Home</h1>
<p><%=homeContent%></p>
    <%posts.forEach(post => { %>
        <div>
         <h1><%=post.title%> </h1>
         <p><%= post.content.slice(0,100)+"..."%>
         <a href="/posts/<%=post.title.replaceAll(" ","-").toLowerCase()%>">Read More</a></p>
        </div>
     <%}); %>
   
<%- include('partials/footer'); %>